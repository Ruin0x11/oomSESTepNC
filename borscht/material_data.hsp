						if ( cActionPeriod(CHARA_PLAYER) == 0 ) {
							gdata(STARTING_GDATA_HELP + 15) = 1
							ghelp = 15
							gosub *elona_help
						}
					}
				}
			}
		}
		if ( locvar_modcorrupt_org + locvar_modcorrupt_mod >= 28 ) {

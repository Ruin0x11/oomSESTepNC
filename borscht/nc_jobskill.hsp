		if ( nc_getskillname(100 + cnt) == "" ) {
			continue
		}
		list(0, listmax) = cnt + 100, cnt + 100
		listn(0, listmax) = nc_getskillname(100 + cnt)
		listn(1, listmax) = nc_getskilldesc(100 + cnt)
		listmax++
	loop
	gosub *sort_list
	redraw 0

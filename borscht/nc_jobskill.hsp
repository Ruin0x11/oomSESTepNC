	listn(0, listmax) = lang("これ以上取得しない", "No more acquisitions")
	listn(1, listmax) = lang("", "")
	listmax++
	if ( nc_intb_001 == 0 | nc_intb_001 == 56 ) {
		repeat 22
			list(0, listmax) = cnt + 1, cnt + 1
			listn(0, listmax) = skillname(911 + cnt)
			listn(1, listmax) = skilldesc(911 + cnt)
			listmax++
		loop
	}
